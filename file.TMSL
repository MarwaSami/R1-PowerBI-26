model Model
  culture: en-US
#Add new line comment
table Sales
  measure SalesAmount = max(Sales[Amount])
table employee
  measure employeeAmount = SUM(employee[Amount])
    formatString: "$ #,##0"

  column 'dateOrder'
    dataType: dateTime
    sourceColumn: OrderDate

  column 'Product Key'
    dataType: int
    sourceColumn: ProductKey
